<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.fingerprintjs.android.fpjs_pro_demo">

    <uses-sdk tools:overrideLibrary="androidx.security.ktx" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.FingerprintJS.Playground">
        <activity
            android:name="com.fingerprintjs.android.fpjs_pro_demo.input_screen.InputScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".results_screen.ResultsActivity"/>
    </application>

</manifest>